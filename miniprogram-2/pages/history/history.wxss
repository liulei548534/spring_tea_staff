.back {
    text-align: right;
}

.back button {
    position: relative;
    top: 20rpx;
    left: -50rpx;
    border-radius: 20rpx;
    display: inline-block;
    width: 250rpx;
    height: 60rpx;
    line-height: 60rpx;
    border: 1px solid #cccccc;
    color: #999999;
    font-size: 30rpx;
}